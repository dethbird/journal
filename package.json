{
  "name": "evidence-journal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node src/web/server.js",
    "ui:dev": "vite --config src/ui/vite.config.js",
    "ui:build": "vite build --config src/ui/vite.config.js",
    "ui:preview": "vite preview --config src/ui/vite.config.js",
    "ui:icons": "node src/ui/generate-icons.js",
    "collector:run": "node src/collector/run.js",
    "digest:run": "node src/digest/run.js",
    "db:dump": "node scripts/db_dump.js",
    "db:restore": "node scripts/db_restore.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:reset": "prisma migrate reset --force",
    "test-db": "node scripts/test-db.js",
    "reset-events": "node scripts/reset-events.js",
    "reset-cursor": "node scripts/reset-cursor.js",
    "pm2:build": "npm run ui:build",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:restart": "pm2 reload ecosystem.config.cjs --env production",
    "pm2:stop": "pm2 stop ecosystem.config.cjs --env production",
    "pm2:logs": "pm2 logs evidence-journal",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status"
  },
  "dependencies": {
    "@fastify/cookie": "^7.4.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/oauth2": "^7.5.0",
    "@fastify/secure-session": "^7.5.1",
    "@fastify/static": "^7.0.2",
    "@mozilla/readability": "^0.5.0",
    "@octokit/rest": "^20.0.0",
    "@prisma/client": "^5.22.0",
    "bulma": "^0.9.4",
    "dotenv": "^16.5.0",
    "fastify": "^4.28.1",
    "gsap": "^3.14.2",
    "imapflow": "^1.0.0",
    "ioredis": "^5.8.2",
    "jsdom": "^24.1.0",
    "juice": "^10.0.0",
    "mailparser": "^3.6.9",
    "marked": "^17.0.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.15",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-swipeable": "^7.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.2",
    "pm2": "^6.0.14",
    "prisma": "^5.22.0",
    "vite": "^5.4.8"
  }
}
